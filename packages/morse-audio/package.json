{
  "name": "morse-audio",
  "version": "1.1.0",
  "description": "Generate morse code audio with radio effects, pileup simulation, and real-time streaming",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "morse",
    "morse-code",
    "audio",
    "cw",
    "ham-radio",
    "amateur-radio",
    "wav",
    "generator",
    "web-audio",
    "streaming",
    "contest",
    "pileup",
    "qrn",
    "qsb",
    "fading",
    "radio-effects",
    "sidetone"
  ],
  "author": "Mark Percival <m@mdp.im>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdp/morse-audio.git",
    "directory": "packages/morse-audio"
  },
  "homepage": "https://github.com/mdp/morse-audio#readme",
  "bugs": {
    "url": "https://github.com/mdp/morse-audio/issues"
  }
}
