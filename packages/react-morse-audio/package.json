{
  "name": "react-morse-audio",
  "version": "1.1.0",
  "description": "React components and hooks for morse code audio with contest simulation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "morse-audio": "^1.1.0"
  },
  "keywords": [
    "react",
    "morse",
    "morse-code",
    "audio",
    "cw",
    "ham-radio",
    "amateur-radio",
    "hook",
    "component",
    "web-audio",
    "streaming",
    "contest",
    "pileup",
    "sidetone",
    "radio-effects"
  ],
  "author": "Mark Percival <m@mdp.im>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdp/morse-audio.git",
    "directory": "packages/react-morse-audio"
  },
  "homepage": "https://github.com/mdp/morse-audio#readme",
  "bugs": {
    "url": "https://github.com/mdp/morse-audio/issues"
  }
}
